{
    "version": 2,
    "projects": {
        "api": {
            "root": "apps/api",
            "sourceRoot": "apps/api/src",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/node:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/api",
                        "main": "apps/api/src/main.ts",
                        "tsConfig": "apps/api/tsconfig.app.json",
                        "assets": []
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/api/src/environments/environment.ts",
                                    "with": "apps/api/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "api:build"
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/api/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/api"],
                    "options": {
                        "jestConfig": "apps/api/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "auth": {
            "root": "libs/auth",
            "sourceRoot": "libs/auth/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/auth/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/auth"],
                    "options": {
                        "jestConfig": "libs/auth/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "client": {
            "root": "apps/client",
            "sourceRoot": "apps/client/src",
            "projectType": "application",
            "targets": {
                "build": {
                    "executor": "@nrwl/web:build",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/client",
                        "index": "apps/client/src/index.html",
                        "main": "apps/client/src/main.tsx",
                        "tsConfig": "apps/client/tsconfig.app.json",
                        "assets": ["apps/client/src/assets"],
                        "styles": [],
                        "scripts": [],
                        "webpackConfig": "@nrwl/react/plugins/webpack"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/client/src/environments/environment.ts",
                                    "with": "apps/client/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "executor": "@nrwl/web:dev-server",
                    "options": {
                        "buildTarget": "client:build",
                        "hmr": true,
                        "proxyConfig": "apps/client/proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "client:build:production",
                            "hmr": false
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/client/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/client"],
                    "options": {
                        "jestConfig": "apps/client/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "client-e2e": {
            "root": "apps/client-e2e",
            "sourceRoot": "apps/client-e2e/src",
            "projectType": "application",
            "targets": {
                "e2e": {
                    "executor": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/client-e2e/cypress.json",
                        "tsConfig": "apps/client-e2e/tsconfig.e2e.json",
                        "devServerTarget": "client:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "client:serve:production"
                        }
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/client-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "data-access-activities": {
            "root": "libs/data-access-activities",
            "sourceRoot": "libs/data-access-activities/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/data-access-activities/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/data-access-activities"],
                    "options": {
                        "jestConfig": "libs/data-access-activities/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "data-access-users": {
            "root": "libs/data-access-users",
            "sourceRoot": "libs/data-access-users/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/data-access-users/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/data-access-users"],
                    "options": {
                        "jestConfig": "libs/data-access-users/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "ui-header": {
            "root": "libs/ui-header",
            "sourceRoot": "libs/ui-header/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/ui-header/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/ui-header"],
                    "options": {
                        "jestConfig": "libs/ui-header/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "ui-tables": {
            "root": "libs/ui-tables",
            "sourceRoot": "libs/ui-tables/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/ui-tables/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/ui-tables"],
                    "options": {
                        "jestConfig": "libs/ui-tables/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "util-guards": {
            "root": "libs/util-guards",
            "sourceRoot": "libs/util-guards/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/util-guards/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/util-guards"],
                    "options": {
                        "jestConfig": "libs/util-guards/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "util-types": {
            "root": "libs/util-types",
            "sourceRoot": "libs/util-types/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/util-types/**/*.ts"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/util-types"],
                    "options": {
                        "jestConfig": "libs/util-types/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "util-ui": {
            "root": "libs/util-ui",
            "sourceRoot": "libs/util-ui/src",
            "projectType": "library",
            "targets": {
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/util-ui/**/*.{ts,tsx,js,jsx}"]
                    }
                },
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/util-ui"],
                    "options": {
                        "jestConfig": "libs/util-ui/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "style": "scss",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "scss"
            },
            "library": {
                "style": "scss",
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "client"
}
