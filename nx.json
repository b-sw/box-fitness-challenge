{
    "npmScope": "nx-nestjs-react-starter",
    "affected": {
        "defaultBase": "main"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "projects": {
        "api": {
            "tags": ["backend"]
        },
        "auth": {
            "tags": []
        },
        "client": {
            "tags": ["frontend"]
        },
        "client-e2e": {
            "tags": ["end-2-end-tests"],
            "implicitDependencies": ["client"]
        },
        "data-access": {
            "tags": []
        },
        "data-access-activities": {
            "tags": []
        },
        "data-access-users": {
            "tags": []
        },
        "feature": {
            "tags": []
        },
        "feature-header": {
            "tags": []
        },
        "ui-header": {
            "tags": []
        },
        "ui-tables": {
            "tags": []
        },
        "util-guards": {
            "tags": []
        },
        "util-types": {
            "tags": ["module"]
        },
        "util-ui": {
            "tags": []
        }
    }
}
